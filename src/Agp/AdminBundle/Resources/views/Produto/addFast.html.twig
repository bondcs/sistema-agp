<form id="ajax-form-fast" novalidate="" class="form" method="POST" action="{{path("produtoAddFast")}}" onsubmit="submitFormFast()">
    {{form_errors(form)}}

    {{ form_row(form.nome)}}
    {{ form_row(form.vlrBase)}}    

    {{ form_row(form._token) }}
    <div class="center">
        <button type="submit" id="submit-form-fast" data-rota="getSelectProdutosAjax" data-select="produto" class="btn pull-left"><i class="icon-ok"></i>Enviar</button>
        <a href="#" class="btn pull-right close-dialog-fast"><i class="icon-off"></i>Fechar</a>
    </div>
</form>

