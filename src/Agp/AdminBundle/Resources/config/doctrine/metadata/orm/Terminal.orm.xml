<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Terminal" table="terminal">
    <id name="codTerminal" type="integer" column="cod_terminal">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="noSerie" type="string" column="no_serie" length="11" nullable="false"/>
    <field name="fabricante" type="string" column="fabricante" nullable="true"/>
    <field name="versao" type="string" column="versao" nullable="true"/>
    <field name="dtEntrada" type="date" column="dt_entrada" nullable="false"/>
    <field name="situacao" type="string" column="situacao" nullable="true"/>
  </entity>
</doctrine-mapping>
