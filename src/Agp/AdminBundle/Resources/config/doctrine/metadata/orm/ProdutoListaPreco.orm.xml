<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ProdutoListaPreco" table="produto_lista_preco">
    <id name="codEmpresa" type="integer" column="cod_empresa"/>
    <id name="codProduto" type="integer" column="cod_produto"/>
    <id name="codListaPreco" type="integer" column="cod_lista_preco"/>
    <field name="vlrProduto" type="float" column="vlr_produto" nullable="false"/>
    <field name="limiteVendas" type="integer" column="limite_vendas" nullable="true"/>
  </entity>
</doctrine-mapping>
